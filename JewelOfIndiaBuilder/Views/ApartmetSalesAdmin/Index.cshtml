        @Html.ActionLink("Create New", "Create")
    </p>*@
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.IsBlocked)
        </th>
        <th>
           Start Date
        </th>
        <th>
            End Date
        </th>
        <th>
           Customer
        </th>
        <th>
            Broker
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Apartment.Description)
        </th>
